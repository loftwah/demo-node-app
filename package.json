{
  "name": "demo-node-app",
  "version": "0.1.0",
  "description": "Loftwah's fun CRUD monkey demo app (Node 20 + TypeScript) for ECS/EKS",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
